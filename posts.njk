---
pagination:
  data: collections
  size: 5
  alias: tag
  filter:
    - all
    - nav
    - post
    - posts
    - tagList
  addAllPagesToCollections: true
layout: layouts/home.njk
eleventyComputed:
  title: Tagged “{{ tag }}”
tags:
  - pages
permalink: /posts/{{ tag | slug }}/
